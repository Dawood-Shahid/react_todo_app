tate('');\r\n    const todoTextHandler = (e) => {\r\n        e.preventDefault();\r\n        setTodoText(e.target.value)\r\n    };\r\n    \r\n    const addTodoHandler = (e) => {\r\n        e.preventDefault();\r\n\r\n        if (todoText.length >= 3) {\r\n            // console.log('addTodo trigered')\r\n            addTodo(todoText)\r\n        }\r\n\r\n        setTodoText('');\r\n    }\r\n\r\n    const useStyles = makeStyles((theme) =>\r\n        createStyles({\r\n            textArea: {\r\n                width: '300px',\r\n                alignItems: 'center',\r\n                justifyItems: 'center',\r\n                padding: '5px',\r\n                display: 'flex',\r\n                margin: '0 auto',\r\n                // border:'1px solid'\r\n            },\r\n        }),\r\n    );\r\n\r\n    const classes = useStyles();\r\n\r\n    return (\r\n        <form\r\n            className={classes.textArea}\r\n            noValidate autoComplete=\"off\"\r\n            onSubmit={addTodoHandler}\r\n        >\r\n            <TextField\r\n                fullWidth={true}\r\n                label=\"Write your todo\"\r\n                value={todoText}\r\n                onChange={todoTextHandler}\r\n            />\r\n            <IconButton\r\n                aria-label='add'\r\n                color='primary'\r\n                onClick={addTodoHandler}\r\n                disabled={todoText.length <= 3}\r\n            >\r\n                <AddIcon />\r\n            </IconButton>\r\n            <IconButton\r\n                aria-label='delete'\r\n                color='secondary'\r\n                onClick={removeTaskList}\r\n            >\r\n                <DeleteIcon fontSize=\"small\" />\r\n            </IconButton>\r\n        </form>\r\n    );\r\n}\r\n\r\nexport default TextArea;\r\n","F:\\Dawood\\Dawood Uni\\Web And Mobile\\Individual\\React\\ReactCode\\react_todo_app\\src\\componets\\todoList\\todo\\Todo.js",["56","57"],"import React, { useContext, useEffect, u