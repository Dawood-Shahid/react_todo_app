            <TextField\r\n                            value={todoText}\r\n                            onChange={editTextHandler}\r\n                        /> :\r\n                        <Typography\r\n                            variant=\"p\"\r\n                            component=\"p\"\r\n                            className='taskDone'\r\n                            className={classesArray.join(' ')}\r\n                        >\r\n                            {todoText}\r\n                        </Typography>\r\n                }\r\n            </div>\r\n            <IconButton\r\n                aria-label='edit'\r\n                color='primary'\r\n                size='small'\r\n                onClick={editFlagHandler}\r\n                disabled={checked}\r\n            >\r\n                <EditIcon />\r\n            </IconButton>\r\n            <IconButton\r\n                aria-label='remove'\r\n                color='secondary'\r\n                size='small'\r\n                onClick={removeTaskHandler}\r\n            >\r\n                <RemoveIcon />\r\n            </IconButton>\r\n        </Paper>\r\n    );\r\n};\r\n\r\nexport default Todo;","F:\\Dawood\\Dawood Uni\\Web And Mobile\\Individual\\React\\ReactCode\\react_todo_app\\src\\componets\\todoList\\TodoList.js",["58"],"import React, { useContext, useEffect } from 'react';\r\nimport TodoContext from '../../context/todoContext/TodoContext';\r\nimport Todo from '../todoList/todo/Todo';\r\nimport {\r\n    Paper,\r\n    Typography,\r\n} from '@material-ui/core';\r\nimport './todoList.css';\r\n\r\nfunction TodoList() {\r\n\r\n    \r\n    const todoContext = useContext(TodoContext);\r\n    const {\r\n        todos,\r\n        getData,\r\n    } = todoContext;\r\n    \r\n    useEffect(() => {\r\n        // console.log('Todo App')\r\n        // getData();\r\n    })\r\n\r\n    const todosArray = Object.keys(todos);\r\n\r\n    return (\r\n        <div className='listBody'>\r\n            {\r\n       